<urlrewrite>
<rule>
	<condition type="request-uri" operator="notequal">api/*</condition>
    <from>^/$</from>
    <to>pages/login.html</to>
</rule>
<rule>
	<condition type="request-uri" operator="notequal">api/*</condition>
	<from>login$</from>
	<to>pages/login.html</to>
</rule>
<rule>
	<condition type="request-uri" operator="notequal">api/*</condition>
	<from>register$</from>
	<to>pages/register.html</to>
</rule>
<rule>
	<from>athleteHomepage$</from>
	<to>pages/homepage_athlete.html</to>
</rule>
<rule>
	<from>trainerHomepage$</from>
	<to>pages/homepage_trainer.html</to>
</rule>
</urlrewrite>
